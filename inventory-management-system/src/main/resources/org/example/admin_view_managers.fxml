<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.AdminViewManagers"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Pane layoutX="228.0" layoutY="25.0" style="-fx-background-color: #2D75E8;" prefHeight="89.0" prefWidth="798.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="managers" layoutX="169.0" layoutY="19.0" text="Managers Table" textFill="white">
                    <font>
                        <Font size="35.0" />
                    </font>
                </Label>
            </children>
        </Pane>
        <TableView fx:id="Table" layoutX="54.0" layoutY="210.0" prefHeight="245.0" prefWidth="702.0" AnchorPane.bottomAnchor="75.0" AnchorPane.leftAnchor="54.0" AnchorPane.rightAnchor="42.0" AnchorPane.topAnchor="210.0">
            <columns>
                <TableColumn fx:id="mid" prefWidth="75.0" text="Manager ID" />
                <TableColumn fx:id="name" prefWidth="75.0" text="Full Name" />
                <TableColumn fx:id="emailid" prefWidth="75.0" text="Email ID" />
                <TableColumn fx:id="contactno" prefWidth="75.0" text="Contact no" />
                <TableColumn fx:id="managertype" prefWidth="75.0" text="Branch" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button fx:id="back" layoutX="522.0" layoutY="25.0" mnemonicParsing="false" onAction="#backtoadminhome" text="Back" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="25.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="remove" layoutX="139.0" layoutY="476.0" mnemonicParsing="false" onAction="#removes" text="Remove" AnchorPane.bottomAnchor="22.0" AnchorPane.leftAnchor="139.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="add" layoutX="54.0" layoutY="476.0" mnemonicParsing="false" onAction="#adds" text="Add" AnchorPane.bottomAnchor="22.0" AnchorPane.leftAnchor="54.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="search" layoutX="734.0" layoutY="108.0" mnemonicParsing="false" onAction="#searchbytype" text="S" AnchorPane.rightAnchor="36.0" AnchorPane.topAnchor="108.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <TextField fx:id="opensthetype" layoutX="536.0" layoutY="108.0" AnchorPane.rightAnchor="76.0" AnchorPane.topAnchor="108.0">
            <font>
                <Font size="15.0" />
            </font>
        </TextField>
        <Label layoutX="374.0" layoutY="109.0" text="Search by Manager Type" AnchorPane.rightAnchor="274.0" AnchorPane.topAnchor="109.0">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button fx:id="clear" layoutX="536.0" layoutY="154.0" mnemonicParsing="false" onAction="#refreshthepage" text="Clear" AnchorPane.rightAnchor="208.0" AnchorPane.topAnchor="154.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
