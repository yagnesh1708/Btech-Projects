<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.AccountsAfterLogin"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Pane layoutX="160.0" style="-fx-background-color: #2D75E8;" layoutY="20.0" prefHeight="107.0" prefWidth="774.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="label" layoutX="154.0" layoutY="28.0" text="Accounts Manager" textFill="white">
                    <font>
                        <Font size="35.0" />
                    </font>
                </Label>
            </children>
        </Pane>
        <Button fx:id="Logout" layoutX="708.0" layoutY="20.0" mnemonicParsing="false" onAction="#gotohomeinv" text="Logout" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="viewbalance" layoutX="160.0" layoutY="150.0" mnemonicParsing="false" onAction="#openbalance" text="View Balance Both" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="160.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="fines" layoutX="160.0" layoutY="227.0" mnemonicParsing="false" onAction="#openfinee" text="View Fine History" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="240.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="transactionshistory" layoutX="160.0" layoutY="300.0" mnemonicParsing="false" onAction="#viewtransactionshistory" text="Purchase History and Repair costs Transactions" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="320.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="fundsaskedbyinv" layoutX="160.0" layoutY="381.0" mnemonicParsing="false" onAction="#openforapprove" text="Fund Requests by Inv Manager" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="400.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="info" layoutX="379.0" layoutY="150.0" mnemonicParsing="false" onAction="#openinfo" text="Info" AnchorPane.leftAnchor="400.0" AnchorPane.topAnchor="160.0">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
